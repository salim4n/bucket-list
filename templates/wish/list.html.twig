{% extends "base.html.twig" %}

{% block headTitle %}
    <h1>Ma liste de souhait</h1>
{% endblock %}

{% block main %}
{% for w in wish %}
    <div>
        <ul>
            <li><h2>{{ w.title }}</h2></li>
            <li><h4><em>{{ w.author }}</em></h4></li>
            <li><p>{{ w.description }}</p></li>
            <li><p>{{ w.dateCreated }}</p></li>
        </ul>
    </div>
{% endfor %}


{% endblock %}


